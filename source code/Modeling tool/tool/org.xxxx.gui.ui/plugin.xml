<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="org.eclipse.ui.internal.handlers.SaveHandler"
            commandId="org.xxxx.gui.ui.commands.save">
      </handler>
      <handler
            class="org.xxxx.gui.ui.handlers.ChangeViewHandler"
            commandId="org.xxxx.gui.ui.commands.changeView">
      </handler>
      <handler
            class="org.xxxx.gui.ui.handlers.CreateGUIHandler"
            commandId="org.xxxx.gui.ui.commands.createGUI">
      </handler>
      <handler
            class="org.xxxx.gui.ui.handlers.CreateMenuHandler"
            commandId="org.xxxx.gui.ui.commands.createMenu">
      </handler>
      <handler
            class="org.xxxx.gui.ui.handlers.CreateInterArchHandler"
            commandId="org.xxxx.gui.ui.commands.createInterArc">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="toolbar:org.eclipse.ui.main.toolbar">
         <toolbar
               id="org.xxxx.gui.ui.menus.toolbar"
               label="User interface design modeling view toolbar">
            <command
                  commandId="org.xxxx.gui.ui.commands.save"
                  label="Save"
                  style="push">
               <visibleWhen
                     checkEnabled="false">
                  <test
                        property="org.xxxx.gui.ui.tester.atGuiPerspective">
                  </test>
               </visibleWhen>
            </command>
            <command
                  commandId="org.xxxx.gui.ui.commands.changeView"
                  label="Back to Start Menu"
                  style="push">
               <visibleWhen
                     checkEnabled="false">
                  <test
                        property="org.xxxx.gui.ui.tester.atGuiPerspective">
                  </test>
               </visibleWhen>
            </command>
            <command
                  commandId="org.xxxx.gui.ui.commands.createMenu"
                  label="New"
                  style="pulldown">
               <visibleWhen
                     checkEnabled="false">
                  <test
                        property="org.xxxx.gui.ui.tester.atGuiPerspective">
                  </test>
               </visibleWhen>
            </command>
         </toolbar>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="menu:org.xxxx.gui.ui.commands.createMenu">
         
         <command
               commandId="org.xxxx.gui.ui.commands.createInterArc"
               label="Interaction architecture diagram"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <test
                     property="org.xxxx.gui.ui.tester.atGuiPerspective">
               </test>
            </visibleWhen>
         </command>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="org.xxxx.gui.ui.perspective.GuiPerspectiveFactory"
            id="org.xxxx.gui.ui.perspective"
            name="GUI Market">
         <description>
            配置用户界面设计视图
         </description>
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            class="org.xxxx.gui.ui.views.navigator.GuiNavigator"
            id="org.xxxx.gui.ui.views.navigator"
            name="Design Modeling Explorer"
            restorable="true">
      </view>
      <view
            class="org.xxxx.gui.ui.views.GuiPropertyView"
            id="org.xxxx.gui.ui.views.property"
            name="Properties"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.navigator.viewer">
      <viewer
            viewerId="org.xxxx.gui.ui.views.navigator">
         <popupMenu
               id="org.xxxx.gui.ui.views.navigator.popupMenu">
            <insertionPoint
                  name="group.edit"
                  separator="false">
            </insertionPoint>
            <insertionPoint
                  name="group.open"
                  separator="false">
            </insertionPoint>
         </popupMenu>
      </viewer>
      <viewerContentBinding
            viewerId="org.xxxx.gui.ui.views.navigator">
         <includes>
            <contentExtension
                  pattern="org.xxxx.gui.ui.views.navigator.content">
            </contentExtension>
         </includes>
      </viewerContentBinding>
   </extension>
   <extension
         point="org.eclipse.ui.navigator.navigatorContent">
      <navigatorContent
            activeByDefault="true"
            contentProvider="org.xxxx.gui.ui.views.navigator.GuiContentProvider"
            id="org.xxxx.gui.ui.views.navigator.content"
            labelProvider="org.xxxx.gui.ui.views.navigator.GuiContentLabelProvider"
            name="GUI Content"
            priority="normal">
         <triggerPoints>
            <instanceof
                  value="org.xxxx.gui.ui.views.navigator.GuiNavigatorRoot">
            </instanceof>
         </triggerPoints>
         <actionProvider
               class="org.xxxx.gui.ui.views.navigator.action.GuiNavigatorActionProvider"
               id="org.xxxx.gui.ui.views.navigator.actions">
         </actionProvider>
         <enablement>
            <or>
               <instanceof
                     value="org.xxxx.gui.ui.views.navigator.GuiNavigatorRoot">
               </instanceof>
               <instanceof
                     value="org.xxxx.gui.ui.views.navigator.nodes.GuiNode">
               </instanceof>
               <instanceof
                     value="org.xxxx.gui.ui.views.navigator.nodes.ComboWidgetNode">
               </instanceof>
               <instanceof
                     value="org.xxxx.gui.ui.views.navigator.nodes.InteractionDiagramNode">
               </instanceof>
               <instanceof
                     value="org.xxxx.gui.ui.views.navigator.collection.GuiCollection">
               </instanceof>
               <instanceof
                     value="org.xxxx.gui.ui.views.navigator.collection.ComboWidgetCollection">
               </instanceof>
               <instanceof
                     value="org.xxxx.gui.ui.views.navigator.collection.InteractionDiagramCollection">
               </instanceof>
            </or>
         </enablement>
      </navigatorContent>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            id="org.xxxx.gui.ui.commands.save"
            name="Save">
      </command>
      <command
            id="org.xxxx.gui.ui.commands.changeView"
            name="Change View">
      </command>
      <command
            id="org.xxxx.gui.ui.commands.createGUI"
            name="Create GUI Diagram">
      </command>
      <command
            id="org.xxxx.gui.ui.commands.createMenu"
            name="Create Menu">
      </command>
      <command
            id="org.xxxx.gui.ui.commands.createInterArc"
            name="Create Inter Arc">
      </command>
   </extension>

</plugin>
